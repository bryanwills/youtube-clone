{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Accept-Encoding",
          "value": "br"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "/author", "destination": "https://github.com/thelissimus" },
    { "source": "/view-source", "destination": "https://github.com/thelissimus/youtube-clone" }
  ],
  "github": {
    "silent": true
  }
}
